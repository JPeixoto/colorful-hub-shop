{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/book/:bookId",
            "destination": "/book/:bookId/index.html"
        },
        {
            "source": "/",
            "has": [
                {
                    "type": "query",
                    "key": "book",
                    "value": "(?<bookId>.*)"
                }
            ],
            "destination": "/book/:bookId"
        }
    ]
}
